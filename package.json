{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon --exitcrash --legacy-watch --config nodemon.json src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "test:watch": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand --watch",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create --config src/common/migrations-orm-config.ts --connection postgresAppConnection --name ",
    "migration:generate": "npm run typeorm -- migration:generate --config src/common/migrations-orm-config.ts --connection postgresAppConnection --name ",
    "migration:run": "npm run typeorm -- migration:run --config src/common/migrations-orm-config.ts --connection postgresAppConnection",
    "migration:revert": "npm run typeorm -- migration:revert --config src/common/migrations-orm-config.ts --connection postgresAppConnection"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "encrypt-password": "^1.0.1",
    "fastify": "^3.24.1",
    "fastify-swagger": "^4.12.6",
    "jsonwebtoken": "^8.5.1",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.4",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "^4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "pg": "^8.7.1",
    "supertest": "6.1.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.3"
  },
  "optionalDependencies": {
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.4.1"
  }
}
